{
  "name": "@interacta/css-labels",
  "version": "0.3.0",
  "description": "CSS Label renderer",
  "author": "Interacta",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.js",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc && cp src/styles.css dist/styles.css",
    "prepublishOnly": "npm run build",
    "watch": "tsc-watch",
    "lint": "eslint \"src/**/*.ts\" --ignore-path .gitignore",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@storybook/addon-docs": "^10.2.4",
    "@storybook/html-vite": "^10.2.4",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-storybook": "^10.2.4",
    "eslint-plugin-unicorn": "^45.0.1",
    "storybook": "^10.2.4",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.9.3"
  },
  "license": "MIT",
  "repository": "git://github.com/interacta-io/css-labels.git",
  "homepage": "https://github.com/interacta-io/css-labels",
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
